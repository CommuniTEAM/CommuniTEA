{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "files.eol": "\n",
  "cSpell.words": [
    "assertjson",
    "codecov",
    "Communi",
    "communitea",
    "DBTX",
    "distro",
    "fkey",
    "golangci",
    "httponly",
    "Iface",
    "Interactor",
    "jackc",
    "jsonbody",
    "jsonschema",
    "jwts",
    "lestrrat",
    "nethttp",
    "nolint",
    "oapi",
    "openapi",
    "pgconn",
    "pgtype",
    "pgxpool",
    "rsvps",
    "ruleset",
    "rulesets",
    "samesite",
    "sqlc",
    "stretchr",
    "swaggest",
    "swgui",
    "testcontainers",
    "Timestamptz",
    "usecase",
    "wikiteadia",
    "zipcode"
  ]
}
